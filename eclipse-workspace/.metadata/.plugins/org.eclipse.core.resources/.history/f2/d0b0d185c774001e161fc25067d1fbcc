package Pack;

public class CoreJava2 {

	public static void main(String[] args) {
		// For loop with IF Else condition
		
		int[] a= {1,9,8,12,44,8};
		
		for(int i=0;i<a.length;i++)
			if(i%2==0)
		{
			System.out.println(a[i]);
	}
			else
			{
				System.out.println(a[i]+"number is odd");
			}
			}
}
